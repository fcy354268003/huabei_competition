<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/chat"
    app:startDestination="@id/friendsFragment2">

    <fragment
        android:id="@+id/friendsFragment2"
        android:name="com.example.huabei_competition.ui.fragments.FriendsFragment"
        android:label="FriendsFragment" >
        <action
            android:id="@+id/action_friendsFragment2_to_applyFragment"
            app:destination="@id/applyFragment" />
        <action
            android:id="@+id/action_friendsFragment2_to_chatFragment"
            app:destination="@id/chatFragment" />
    </fragment>
    <fragment
        android:id="@+id/applyFragment"
        android:name="com.example.huabei_competition.ui.fragments.ApplyFragment"
        android:label="ApplyFragment" />
    <fragment
        android:id="@+id/chatFragment"
        android:name="com.example.huabei_competition.ui.fragments.ChatFragment"
        android:label="fragment_chat"
        tools:layout="@layout/fragment_chat" />
</navigation>